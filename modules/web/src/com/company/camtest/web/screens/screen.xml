<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://caption"
        class="com.company.camtest.web.screens.Screen"
        messagesPack="com.company.camtest.web.screens">
    <dialogMode height="600"
                width="800"/>
    <layout expand="tabsheet" margin="true">
        <tabSheet id="tabsheet">
            <tab id="camTab" caption="cam">
                <vbox id="box" width="100%" height="300px"/>
            </tab>
            <tab id="labelTab" caption="label">
                <label id="label" value="label"/>
            </tab>
        </tabSheet>
    </layout>
</window>
